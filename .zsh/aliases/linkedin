alias ssh-proxy-on="cp ~/.ssh/config.proxy ~/.ssh/config.custom"
alias ssh-proxy-off="cp ~/.ssh/config.direct ~/.ssh/config.custom"
alias notes="code ~/OneDrive\ -\ Microsoft/Notes"
alias aws-glint-prod="export AWS_PROFILE=glint-prod && ln -s ~/.aws/config.deactivated ~/.aws/config"
alias aws-glint-dev="export AWS_PROFILE=glint-dev && ln -s ~/.aws/config.deactivated ~/.aws/config"
alias aws-glint-deactivate="unset AWS_PROFILE && rm ~/.aws/config 2>/dev/null"
alias az-glint-prod="az account set --subscription a997adba-cb33-4c62-966d-723795a7e5dc && export AZ_GLINT_PROFILE=prod"
alias az-glint-dev="az account set --subscription d453c562-8ac3-44e7-bebe-9c0e51c8f3a6 && export AZ_GLINT_PROFILE=dev"
alias az-glint-deactivate="unset AZ_GLINT_PROFILE"
